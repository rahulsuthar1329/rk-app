{
  "expo": {
    "name": "rk",
    "scheme": "https",
    "slug": "rk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rk-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/rk-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rk1329.rk",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.rkenterprisy.com"
            }
          ],
          "category": ["LAUNCHER", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c361985b-af7c-4c57-9716-f917950f09e4"
      }
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow GenieCart to access your microphone."
        }
      ]
    ]
  }
}
